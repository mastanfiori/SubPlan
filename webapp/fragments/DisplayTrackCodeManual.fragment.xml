<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<Dialog title="{i18n>ManualTrackingCodesHeader}" contentWidth="40%">
		<content>
			<Table sticky="ColumnHeaders" alternateRowColors="true" items="{manualTrackCodesDisplay>/}" mode="None">
				<columns>
					<Column>
						<Label text="{i18n>TrackingCode}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem>
						<cells>
							<Text text="{manualTrackCodesDisplay>TrackingCode}"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
		<endButton>
			<Button text="Cancel" press="onDisplayDialogCancel"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>